{
    "rules": {
        "no-cond-assign"             : 2,
        "no-console"                 : 1,
        "no-constant-condition"      : 2,
        "no-control-regex"           : 1,
        "no-debugger"                : 1,
        "no-dupe-keys"               : 2,
        "no-dupe-args"               : 2,
        "no-duplicate-case"          : 2,
        "no-empty"                   : 2,
        "no-empty-character-class"   : 2,
        "no-ex-assign"               : 2,
        "no-extra-boolean-cast"      : 1,
        "no-extra-parens"            : 1,
        "no-extra-semi"              : 1,
        "no-func-assign"             : 2,
        "no-inner-declarations"      : 2,
        "no-invalid-regexp"          : 2,
        "no-irregular-whitespace"    : 2,
        "no-negated-in-lhs"          : 2,
        "no-obj-calls"               : 2,
        "no-regex-spaces"            : 2,
        "no-reserved-keys"           : 2,
        "no-sparse-arrays"           : 1,
        "no-unreachable"             : 2,
        "use-isnan"                  : 1,
        "valid-jsdoc"                : 1,
        "valid-typeof"               : 2,
        "no-unexpected-multiline"    : 2,
        "accessor-pairs"             : 1,
        "block-scoped-var"           : 2,
        "complexity"                 : [1, 2],
        "consistent-return"          : 1,
        "curly"                      : [2, "all"],
        "default-case"               : 2,
        "dot-notation"               : 1,
        "eqeqeq"                     : [2, "smart"],
        "guard-for-in"               : 2,
        "no-alert"                   : 2,
        "no-caller"                  : 2,
        "no-div-regex"               : 2,
        "no-else-return"             : 2,
        "no-empty-label"             : 0,
        "no-eq-null"                 : 2,
        "no-eval"                    : 2,
        "no-extend-native"           : 2,
        "no-extra-bind"              : 2,
        "no-fallthrough"             : 1,
        "no-floating-decimal"        : 2,
        "no-implied-eval"            : 2,
        "no-iterator"                : 2,
        "no-labels"                  : 2,
        "no-lone-blocks"             : 2,
        "no-loop-func"               : 2,
        "no-multi-spaces"            : 2,
        "no-multi-str"               : 2,
        "no-native-reassign"         : 2,
        "no-new"                     : 2,
        "no-new-func"                : 2,
        "no-new-wrappers"            : 2,
        "no-octal"                   : 2,
        "no-octal-escape"            : 2,
        "no-param-reassign"          : 2,
        "no-process-env"             : 2,
        "no-proto"                   : 2,
        "no-redeclare"               : 2,
        "no-return-assign"           : 2,
        "no-script-url"              : 2,
        "no-self-compare"            : 2,
        "no-sequences"               : 2,
        "no-throw-literal"           : 1,
        "no-unused-expressions"      : 2,
        "no-void"                    : 2,
        "no-warning-comments"        : 1,
        "no-with"                    : 2,
        "radix"                      : 1,
        "vars-on-top"                : 2,
        "wrap-iife"                  : 2,
        "yoda"                       : [2, "never"],
        "strict"                     : [1, "global"],
        "no-catch-shadow"            : 2,
        "no-delete-var"              : 2,
        "no-label-var"               : 2,
        "no-shadow"                  : 2,
        "no-shadow-restricted-names" : 2,
        "no-undef"                   : 2,
        "no-undef-init"              : 2,
        "no-undefined"               : 2,
        "no-unused-vars"             : 2,
        "no-use-before-define"       : 2,
        "handle-callback-err"        : 2,
        "no-mixed-requires"          : 0,
        "no-new-require"             : 2,
        "no-path-concat"             : 2,
        "no-process-exit"            : 2,
        "no-restricted-modules"      : 0,
        "no-sync"                    : 0,
        "arrays-bracket-spacing"     : [2, "never"],
        "brace-style"                : [2, "1tbs", { "allowSingleLine": false }],
        "camelcase"                  : 2,
        "comma-spacing"              : [2, {"before": false, "after": true}],
        "comma-style"                : [2, "last"],
        "computed-property-spacing"  : [2, "never"],
        "consistent-this"            : [2, "that"],
        "eol-last"                   : 2,
        "func-names"                 : 1,
        "func-style"                 : [2, "expression"],
        "indent"                     : [2, 2],
        "key-spacing"                : [2, {"beforeColon": false, "afterColon": true, "align": "colon"}],
        "lines-around-comment"       : [2, {"beforeBlockComment": true, "afterBlockComment": true, "beforeLineComment": true, "allowBlockStart": true, "allowBlockEnd": true}],
        "max-nested-callbacks"       : [2, 2],
        "new-cap"                    : 2,
        "new-parens"                 : 2,
        "newline-after-var"          : [2, "always"],
        "no-array-constructor"       : 2,
        "no-continue"                : 2,
        "no-inline-comments"         : 0,
        "no-lonely-if"               : 2,
        "no-mixed-spaces-and-tabs"   : 2,
        "no-multiple-empty-lines"    : [2, 3],
        "no-nested-ternary"          : 2,
        "no-new-object"              : 2,
        "no-spaced-func"             : 2,
        "no-ternary"                 : 0,
        "no-trailing-spaces"         : 2,
        "no-underscore-dangle"       : 2,
        "no-unneeded-ternary"        : 2,
        "object-curly-spacing"       : [2, "never"],
        "one-var"                    : [2, "never"],
        "operator-assignment"        : [1, "always"],
        "operator-linebreak"         : [2, "after"],
        "padded-blocks"              : 0,
        "quote-props"                : [2, "as-needed"],
        "quotes"                     : [2, "single", "avoid-escape"],
        "semi"                       : [2, "always"],
        "sort-vars"                  : 0,
        "space-after-keywords"       : [2, "always"],
        "space-before-blocks"        : [2, "always"],
        "space-in-parens"            : [2, "never"],
        "space-infix-ops"            : 2,
        "space-return-throw-case"    : 2,
        "space-unary-ops"            : [2, {"words": true, "nonwords": false}],
        "spaced-comment"             : [2, "always"],
        "wrap-regex"                 : 0,
        "constructor-super"          : 0,
        "generator-star-spacing"     : 0,
        "no-this-before-super"       : 0,
        "no-var"                     : 0,
        "object-shorthand"           : 0,
        "prefer-const"               : 0
    },
    "env": {
        "es6"    : true,
        "browser": true,
        "node"   : true
    }
}